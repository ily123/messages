{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": 11
    },
    "rules": {
        "semi": [2, "always"],
        "indent": ["error", 2],
        "max-len": ["error", {"code": 88}]
    }
}
